{
  "name": "ra-feathers-client",
  "version": "0.9.3",
  "description": "A feathers client for react-admin",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run coverage",
    "build": "cross-env NODE_ENV=production babel ./src -d lib --ignore '*.spec.js'",
    "prepublish": "npm run build",
    "clean": "rm -rf lib",
    "publish": "git push origin --tags && git push origin",
    "changelog": "github_changelog_generator && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts",
    "mocha": "cross-env DEBUG=aor-feathers-client:* mocha"
  },
  "files": [
    "LICENSE",
    "*.md",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spaceindex/ra-feathers-client.git"
  },
